{
  "name": "gordon-360-ui",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": ">=1.4.1",
    "@material-ui/icons": ">=1.1.0",
    "chart.js": ">=2.7.1",
    "classnames": ">=2.2.5",
    "cropperjs": ">=1.4.1",
    "downshift": ">=1.22.3",
    "history": ">=4.7.2",
    "http-proxy-middleware": ">=0.19.1",
    "jwt-decode": ">=2.2.0",
    "lodash": ">=4.17.11",
    "luxon": ">=0.2.12",
    "prop-types": ">=15.6.0",
    "react": ">=16.2.0",
    "react-chartjs-2": ">=2.7.4",
    "react-cropper": ">=1.0.1",
    "react-document-title": ">=2.0.3",
    "react-dom": ">=16.2.0",
    "react-dropzone": ">=10.1.5",
    "react-ga": ">=2.3.5",
    "react-graceful-image": ">=1.2.9",
    "react-icons": ">=2.2.7",
    "react-media": ">=1.8.0",
    "react-responsive-carousel": ">=3.1.30",
    "react-router-dom": ">=4.2.2",
    "typescript": ">=3.5.3"
  },
  "devDependencies": {
    "eslint-config-react-app": ">=2.0.1",
    "eslint-plugin-flowtype": ">=2.40.1",
    "eslint-plugin-import": ">=2.8.0",
    "eslint-plugin-jsx-a11y": ">=5.1.1",
    "eslint-plugin-react": ">=7.5.1",
    "husky": ">=0.14.3",
    "jasmine-core": ">=3.4.0",
    "jsdoc": ">=3.6.2",
    "karma": ">=4.1.0",
    "karma-chrome-launcher": ">=2.2.0",
    "karma-firefox-launcher": ">=1.1.0",
    "karma-jasmine": ">=2.0.1",
    "karma-safari-launcher": ">=1.0.0",
    "lint-staged": ">=6.0.0",
    "node-sass-chokidar": ">=1.3.5",
    "npm-run-all": ">=4.1.2",
    "prettier": ">=1.9.2",
    "react-scripts": ">=3.0.1",
    "stylelint": ">=10.1.0",
    "stylelint-config-recommended-scss": ">=3.0.0",
    "stylelint-scss": ">=2.2.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch",
    "start-js": "react-scripts start",
    "start": "npm-run-all --parallel watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "docs": "jsdoc --configure .jsdoc.json --recurse",
    "format": "prettier --write '{**/*.md,src/**/*.{js,scss}}'",
    "lint": "npm-run-all --parallel --aggregate-output --silent lint-js lint-scss",
    "lint-js": "eslint --ext .js src",
    "lint-scss": "stylelint 'src/**/*.scss'",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "{**/*.md,src/**/*.{js,scss}}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.js": [
      "eslint",
      "git add"
    ],
    "src/**/*.scss": [
      "stylelint",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
