  "devDependencies": {
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.7.2",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "husky": "^6.0.0",
    "jsdoc": "^3.6.7",
    "lint-staged": "11.0.0",
    "prettier": "^2.3.0",
    "react-scripts": "^4.0.3",
    "sass": "^1.42.1",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended-scss": ">=4.2.0",
    "stylelint-scss": "^3.19.0"
  },
  "scripts": {
    "build-css": "sass src --no-source-map",
    "watch-css": "sass --watch src",
    "start-js": "react-scripts start",
    "dev": "(start npm run watch-css && npm run start-js) || (npm run watch-css & npm run start-js)",
    "start": "npm run dev",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "docs": "jsdoc --configure .jsdoc.json --recurse",
    "format": "prettier --write '{**/*.md,src/**/*.{js,scss}}'",
    "lint": "npm run --silent lint-js && npm run --silent lint-scss",
    "lint-js": "eslint --ext .js src",
    "lint-scss": "stylelint 'src/**/*.scss'",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "{**/*.md,src/**/*.{js,scss}}": "prettier --write",
    "src/**/*.js": "eslint",
    "src/**/*.scss": "stylelint"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
